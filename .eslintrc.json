{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:vue/essential",
        "eslint:recommended"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "indent": [
            "error",
            2,
            { "SwitchCase": 1 }
        ],
        "linebreak-style": [
            "error",
            "unix"
        ],
        "switch-colon-spacing": ["error"],
        "default-case": "error"
//        "quotes": [
//            "error",
//            "single"
//        ],
//        "semi": [
//            "error",
//            "always",
//            { "omitLastInOneLineBlock": true}
//        ]
    }
}
