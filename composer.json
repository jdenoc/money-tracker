{
    "name": "jdenoc/money-tracker",
    "description": "income/expense tracker with receipt retention",
    "homepage": "https://github.com/jdenoc/money-tracker",
    "keywords": ["laravel", "finance"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "ext-pdo": "*",
        "beyondcode/laravel-favicon": "dev-master as 1.3.0",
        "doctrine/dbal": "2.13.2",
        "eklundkristoffer/laravel-discord-webhook": "dev-master",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.3.1|^7.0.1",
        "jdenoc/laravel-app-version": "^1.1",
        "jdenoc/laravel-make-trait": "^v1.1",
        "laravel/framework": "^7.29",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "2.6.1",
        "laravel/ui": "^2.0",
        "maatwebsite/excel": "^3.1",
        "nesbot/carbon": "2.46.0",
        "spatie/laravel-db-snapshots": "^2.2"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "beyondcode/laravel-masked-db-dump": "dev-main",
        "facade/ignition": "^2.0",
        "fakerphp/faker": "1.15.0",
        "filp/whoops": "2.12.1",
        "jdenoc/bulma-colors": "0.0.2",
        "laravel/dusk": "6.15.0",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3",
        "phpunit/php-invoker": "^3.1",
        "phpunit/phpunit": "^8.5.8|^9.3.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta",
            "php artisan ide-helper:eloquent"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true,
        "platform": {
            "php": "8.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jdenoc/laravel-discord-webhook"
        },
        {
            "type": "vcs",
            "url": "https://github.com/eminos/laravel-favicon"
        },
        {
            "type": "vcs",
            "url": "https://github.com/marispro/laravel-masked-db-dump"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
