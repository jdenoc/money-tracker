{
    "name": "jdenoc/money-tracker",
    "description": "income/expense tracker with receipt retention",
    "keywords": ["laravel"],
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "ext-json": "*",
        "doctrine/dbal": "^2.5",
        "eklundkristoffer/laravel-discord-webhook": "dev-master",
        "fideloper/proxy": "^4.0",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel/framework": "5.8.*",
        "laravel/helpers": "^1.2",
        "laravel/tinker": "^1.0",
        "nesbot/carbon": "2.27.0 as 1.39.0",
        "ramsey/uuid": "^3.6"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "^2.2",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "laravel/dusk": "^4.0",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "spatie/db-dumper": "2.13.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [{
        "type": "vcs",
        "url": "https://github.com/jdenoc/laravel-discord-webhook"
    }],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
