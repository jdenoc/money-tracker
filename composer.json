{
    "name": "jdenoc/money-tracker",
    "description": "income/expense tracker with receipt retention",
    "homepage": "https://github.com/jdenoc/money-tracker",
    "keywords": ["laravel", "finance"],
    "type": "project",
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "ext-pdo": "*",
        "beyondcode/laravel-favicon": "1.3.0",
        "doctrine/dbal": "2.12.1",
        "eklundkristoffer/laravel-discord-webhook": "dev-master",
        "fakerphp/faker": "1.13.0",
        "fideloper/proxy": "4.4.1",
        "jdenoc/laravel-app-version": "1.0.2",
        "jdenoc/laravel-make-trait": "1.0.1",
        "laravel/framework": "6.20.16",
        "laravel/helpers": "1.4.1",
        "laravel/tinker": "2.6.0",
        "nesbot/carbon": "2.45.1",
        "ramsey/uuid": "3.9.3"
    },
    "require-dev": {
        "barryvdh/laravel-ide-helper": "2.8.2",
        "beyondcode/laravel-dump-server": "1.3.0",
        "filp/whoops": "2.9.2",
        "laravel/dusk": "5.11.0",
        "mockery/mockery": "1.4.3",
        "nunomaduro/collision": "3.2.0",
        "phpunit/phpunit": "8.5.14",
        "spatie/db-dumper": "2.21.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-update-cmd": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\"",
            "@php artisan key:generate --ansi"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "ide-helper": [
            "php artisan ide-helper:generate",
            "php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jdenoc/laravel-discord-webhook"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/dusk"
            ]
        }
    }
}
